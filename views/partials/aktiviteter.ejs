<h1><%- aktivitetertitle -%></h1>

<div class="activity">
    <% for (let i =0; i < activity.length;  i++ ) { %>
    <div>
        <h2><%- activity[i].name -%></h2>
        <p><%- activity[i].friendly_name -%></p>
        <p><%- activity[i].classroom -%></p>
        <p><%- activity[i].class -%></p>
        <p><%- activity[i].time -%></p>
    </div>
    <% } %>
</div>